       return SendClientMessage(playerid, COLOR_GRAD1, "You're not authorized to use that command!");

	}

	new id;

	if(sscanf(params, "d", id)) return SendClientMessage(playerid, COLOR_GREY, "YCMD: /deletebiz [id]");
