			return SendClientMessage(playerid, COLOR_GRAD1, "The specified pay amount must be between $1 and $800.");
		}

		FactionPay[iFactionID][iRankValue] = iPayValue;

		new
			szMessage[46 + MAX_PLAYER_NAME];

