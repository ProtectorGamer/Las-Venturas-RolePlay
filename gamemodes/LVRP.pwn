	}
	new
	    Float: fVehicleHealth,
		iCount;

	for(new d = 0 ; d < MAX_PLAYERVEHICLES; d++) {
		if(IsPlayerInVehicle(playerid, PlayerVehicleInfo[playerid][d][pvId])) {
