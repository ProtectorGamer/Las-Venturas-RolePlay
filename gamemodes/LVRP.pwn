			return 1;
	    }

		new
			iFactionID,
			iRankValue,
			iPayValue;

		if(sscanf(params, "ddd", iFactionID, iRankValue, iPayValue)) {
			return SendClientMessage(playerid, COLOR_WHITE, "USAGE: /facpay [faction ID] [rank] [pay amount]");
