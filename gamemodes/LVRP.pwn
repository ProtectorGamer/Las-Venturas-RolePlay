stock DoubleEXPcheck(hour) {
    FixHour(hour);
    hour = shifthour;

	if(hour >= 13 && hour <= 15)
		return 1; // EU timezone
	else if(hour >= 18 && hour <= 20)
		return 1; // US timezone
    else if(hour >= 0 && hour <= 2)
		return 1; // OC timezone

	return 0;
}

