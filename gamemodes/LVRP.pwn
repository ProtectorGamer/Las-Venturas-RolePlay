			szMessage[128],
			szRank[64],
			szEmployer[64],
			szDivision[64];
		SendClientMessage(playerid, COLOR_NEWS, "Members Online:");
		foreach(Player, i) {
			if(PlayerInfo[playerid][pFaction] == PlayerInfo[i][pFaction] && AdminDuty[i] != 1) {
				GetPlayerFactionInfo(i, szRank, szDivision, szEmployer);
				if(PlayerInfo[i][pDivision] != 0) {
