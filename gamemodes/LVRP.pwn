			VehicleRadioStation[PlayerVehicleInfo[playerid][v][pvId]] = 0;
			//VehicleFuel[PlayerVehicleInfo[playerid][v][pvId]] = 100.0;
			PlayerVehicleInfo[playerid][v][pvId] = INVALID_PLAYER_VEHICLE_ID;
