				format(string, 128, "AdmCmd: %s was offline prisoned by %s, reason: %s", name, GetPlayerNameEx(playerid), reason);
				ABroadCast(COLOR_LIGHTRED,string,1);
			}
		}
		else
		{
			SendClientMessage(playerid, COLOR_WHITE, "That account doesn't exist.");
		}
	}
	return 1;
}

