		if(IsPlayerConnected(giveplayerid))
		{
			new fine, allmoney;

			if(PlayerInfo[giveplayerid][pHouse] != INVALID_HOUSE_ID && strcmp(GetPlayerNameEx(playerid), HouseInfo[PlayerInfo[giveplayerid][pHouse]][hOwner], true) == 0)
