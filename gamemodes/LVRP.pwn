			    return SendClientMessage(playerid, COLOR_GREY, "The license plate can not be longer than 8 characters!");

			if(strcmp(color, "black", true)==0) format(PlayerVehicleInfo[playerid][d][pvNumberPlate], 32, "{000000}%s", plate);
