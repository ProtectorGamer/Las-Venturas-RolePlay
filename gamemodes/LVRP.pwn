	}

    new v;
	foreach(Player, i)
	{
	    v = GetPlayerVehicle(i, vehicleid);
	    if(v != -1)
	    {
 			if(PlayerVehicleInfo[i][v][pvId] == vehicleid && GetVehicleModel(vehicleid) != PlayerVehicleInfo[i][v][pvModelId])
	  		{
	   			DestroyVehicle(vehicleid);
